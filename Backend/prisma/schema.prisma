// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Depot {
  id        Int      @id @default(autoincrement())
  name      String
  city      String
  location  String
  products  String[]
  capacity  Int
  state     String
}

model Feed {
  id       Int      @id @default(autoincrement())
  title    String
  subtitle String
  image    String
  date     DateTime
  layout   String
}

model Items {
  id       Int      @id @default(autoincrement())
  Type    String
  Price    String
  image    String
  Description     DateTime
}
